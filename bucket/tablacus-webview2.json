{
    "version": "22.3.15",
    "homepage": "https://tablacus.github.io/tewv2_ja.html",
    "description": "Tablacus Explorer Blink extension",
    "license": "MIT",
    "url": "https://github.com/tablacus/TablacusExplorerWebView2/releases/download/22.3.15/tewv2_220315.zip",
    "hash": "5a7953e44b5a1dccb88d2be0b842b764115b7a485f08d4ea0571f901c6fa5f7f",
    "depends": "furry-potato/tablacus-explorer",
    "extract_dir": "tewv",
    "pre_install": [
        "$ErrorActionPreference = \"silentlycontinue\"",
        "$pro = Get-Process \"TE\"",
        "if ( $pro -ne $null ) {",
        "    Write-Output \"Stopping Tablacus Explorer...\"",
        "    Stop-process -name \"TE\" -Force",
        "}",
        "while ( $pro -ne $null ) {",
        "    $pro = Get-Process \"TE\"",
        "}"
    ],
    "installer": {
        "script": [
            "Copy-Item \"$dir\\lib\\tewv*.dll\" \"$(appdir tablacus-explorer $global)\\current\\lib\" -Recurse -Force"
        ]
    },
    "post_install": [
        "Write-Output \"Starting Tablacus Explorer...\"",
        "Start-Process -FilePath \"$(appdir tablacus-explorer $global)\\current\\TE.exe\"",
        "while ( $pro -eq $null ) {",
        "    $pro = Get-Process \"TE\"",
        "}"
    ],
    "uninstaller": {
        "script": [
            "$ErrorActionPreference = \"silentlycontinue\"",
            "$pro = Get-Process \"TE\";",
            "if ( $pro -ne $null ) {",
            "    Write-Output \"Stopping Tablacus Explorer...\"",
            "    Stop-process -name \"TE\" -Force",
            "}",
            "while ( $pro -ne $null ) {",
            "    $pro = Get-Process \"TE\"",
            "}",
            "Remove-Item \"$(appdir tablacus-explorer $global)\\current\\lib\\tewv*.dll\" -Recurse -Force"
        ]
    },
    "checkver": {
        "url": "https://github.com/tablacus/TablacusExplorerWebView2/releases/latest",
        "regex": "/releases/download/([\\d.]+)/tewv2_(?<cleanpadding>[\\d]+).zip"
    },
    "autoupdate": {
        "url": "https://github.com/tablacus/TablacusExplorerWebView2/releases/download/$version/tewv2_$matchCleanpadding.zip"
    }
}
