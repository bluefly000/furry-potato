    "bin": "TE.exe",
    "shortcuts": [
        [
            "TE.exe",
            "Tablacus Explorer"
        ]
    ],
    "persist": [
        "addons",
        "config",
        "icons"
    ],
    "checkver": {
        "regex": "Version <span id=\"ver\">([\\d.]+)</span>[\\w\\W]*?te(?<cleanpadding>\\d+)\\.zip"
    },
    "autoupdate": {
        "url": "https://github.com/tablacus/TablacusExplorer/releases/download/$version/te$matchCleanpadding.zip"
